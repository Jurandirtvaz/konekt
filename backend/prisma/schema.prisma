generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") //Usaremos para queries r√°pidas
  directUrl = env("DIRECT_URL") //Usaremos APENAS para migrations
}

//Model para teste
model User {
  id    String @id @default(uuid())
  name  String
  email String @unique
  
  @@map("users")
}